    <main>
      <div id="contact">
        <img src="/img/contact/contactimg.webp" alt="" id="contactimg" />
        <form
          action="https://api.web3forms.com/submit"
          method="post"
          id="contactform"
        >
          <input
            type="hidden"
            name="access_key"
            value="2cbca988-3f23-4ea5-b19e-ca966832406f"
          />
          <input type="text" name="name" placeholder="Name" required />

          <input type="email" name="enail" placeholder="Email" required />

          <select id="jewelry-type" name="jewelry-type">
            <option value="necklace">Necklace</option>
            <option value="bracelet">Bracelet</option>
            <option value="ring">Ring</option>
            <option value="earrings">Earrings</option>
            <option value="anklet">Lockets</option>
          </select>

          <input type="text" name="size" placeholder="Size" />
          <textarea
            name="textarea"
            id="textarea"
            placeholder="Add your requirements and wishes, we are glad to fulfill them!"
          ></textarea>

          <input
            type="number"
            name="number"
            placeholder="Phone Number"
            id="phonenumber"
          />
          <button type="submit" name="submit" id="submitbtn">Submit</button>

          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3022.1833141233246!2d-73.9803888!3d40.7579927!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c258ff1c7b3827%3A0x5fa09255f1c4696e!2s48%20W%2048th%20St%20%231008%2C%20New%20York%2C%20NY%2010036%2C%20USA!5e0!3m2!1sen!2sin!4v1727816070185!5m2!1sen!2sin"
            style="border: 0"
            allowfullscreen=""
            loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"
          ></iframe>
        </form>
      </div>
